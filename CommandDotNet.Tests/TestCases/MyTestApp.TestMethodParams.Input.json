{
    "testCases": [
        {
            "testCaseName": "can recognise string, int, string array, double & few null entries",
            "params" : [
                "TestMethodParams",
                "--name", "\"bilal fazlani\"",
                "--id", "4",
                "--tag", "human",
                "-t", "male",
                "--tag", "4",
                "--height", "5.2",
                "--email", "true",
                "--category", "b"
            ],
            "expectedOutput": {
                "name" : "bilal fazlani",
                "id" : 4,
                "tags" : ["human", "male", "4"],
                "height" : 5.2,
                "city" : null,
                "nri" : false,
                "roles" : null,
                "ranks" : null,
                "email" : true,
                "sms": false,
                "category" : "b"
            }
        },
        {
            "testCaseName": "can recognise all default values",
            "params" : [
                "TestMethodParams"
            ],
            "expectedOutput": {
                "name" : null,
                "id" : 0,
                "tags" : null,
                "height" : 0.0,
                "city" : null,
                "nri" : false,
                "roles" : null,
                "ranks" : null,
                "email" : false,
                "sms" : false,
                "category" : "\u0000"
            }
        },
        {
            "testCaseName": "can recognise flags",
            "params" : [
                "TestMethodParams",
                "--sms"
            ],
            "expectedOutput": {
                "name" : null,
                "id" : 0,
                "tags" : null,
                "height" : 0.0,
                "city" : null,
                "nri" : false,
                "roles" : null,
                "ranks" : null,
                "email" : false,
                "sms" : true,
                "category" : "\u0000"
            }
        }
    ]
}
